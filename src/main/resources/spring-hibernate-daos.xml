<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <bean id="accountDao" class="de.iew.framework.persistence.hibernate.HbmAccountDaoImpl"/>

    <bean id="auditEventMessageDao" class="de.iew.framework.persistence.hibernate.HbmAuditEventMessageDaoImpl"/>

    <bean id="authorityDao" class="de.iew.framework.persistence.hibernate.HbmAuthorityDaoImpl"/>

    <bean id="messageDao" class="de.iew.framework.persistence.hibernate.HbmMessageBundleDaoImpl"/>

    <bean id="nodeDao" class="de.iew.framework.persistence.hibernate.HbmNodeDaoImpl"/>

