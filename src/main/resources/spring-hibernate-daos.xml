    <bean id="treeDao" class="de.iew.framework.persistence.hibernate.HbmTreeDaoImpl"/>

    <bean id="treeOperationDao" class="de.iew.framework.persistence.hibernate.HbmTreeOperationDaoImpl"/>

    <bean id="webResourceDao" class="de.iew.framework.persistence.hibernate.HbmWebResourceDaoImpl"/>

    <bean id="dataSourceDao" class="de.iew.framework.persistence.hibernate.HdmDataSourceDaoImpl"/>

